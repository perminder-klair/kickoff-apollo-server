{
  "name": "kickoff-apollo-server",
  "version": "2.0.1",
  "description": "A very lean boilerplate for setting up GraphQL server.",
  "main": "dist/index.js",
  "keywords": [
    "graphql",
    "apollo",
    "apollo server",
    "apollo graphql"
  ],
  "author": "Parminder Klair",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon --exec babel-node -- ./src/index.js",
    "eslint": "eslint --fix --ext .jsx,.js ./src/**",
    "build-email": "node ./src/build-emails.js",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && babel ./src --out-dir dist --copy-files",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.180.0",
    "@aws-sdk/client-s3": "^3.180.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.180.0",
    "@aws-sdk/s3-request-presigner": "^3.180.0",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@graphql-tools/merge": "^8.3.6",
    "@graphql-tools/schema": "^9.0.4",
    "apollo-server": "^3.10.2",
    "apollo-server-core": "^3.10.2",
    "bcryptjs": "^2.4.3",
    "convict": "^6.2.3",
    "dotenv": "^16.0.2",
    "email-templates": "^10.0.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mjml": "^4.13.0",
    "mongoose": "^6.6.1",
    "nodemailer": "^6.8.0",
    "randomstring": "^1.2.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/node": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/runtime": "^7.19.0",
    "@graphql-eslint/eslint-plugin": "^3.11.2",
    "babel-plugin-graphql-tag": "^3.3.0",
    "babel-plugin-import-graphql": "^2.8.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1"
  }
}
