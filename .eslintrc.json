{
  "plugins": ["prettier", "import"],
  "extends": ["plugin:prettier/recommended", "airbnb-base"],
  "rules": {
    "quotes": 0,
    "import/prefer-default-export": 0,
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "all",
      "bracketSpacing": true,
      "jsxBracketSameLine": false,
      "parser": "flow"
    }]
  }
}
